{
  "name": "@bayfrontmedia/skin",
  "version": "3.0.1",
  "description": "A free open source library of Tailwind CSS components.",
  "author": "Bayfront Media",
  "license": "MIT",
  "homepage": "https://github.com/bayfrontmedia/skin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bayfrontmedia/skin.git"
  },
  "bugs": {
    "url": "https://github.com/bayfrontmedia/skin/issues"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "tailwind css",
    "tailwind library",
    "component",
    "ui",
    "skin",
    "css"
  ],
  "main": "dist/js/skin.js",
  "scripts": {
    "app:update": "npm update && cp node_modules/preline/dist/preline.js docs/assets/js/preline.js",
    "watch:styles": "npx @tailwindcss/cli -i src/css/styles.css -o docs/assets/css/styles.css --watch",
    "build:styles": "npx @tailwindcss/cli -i src/css/styles.css -o docs/assets/css/styles.css",
    "build:styles:prod": "npx @tailwindcss/cli -i src/css/styles.css -o docs/assets/css/styles.css --minify",
    "watch:webpack": "webpack --mode development --watch",
    "build:webpack": "webpack --mode development",
    "build:webpack:prod": "webpack --mode production",
    "build:prod": "npm run app:update && npm run build:webpack:prod && npm run build:styles:prod && cp src/css/skin.css dist/css/skin.css"
  },
  "devDependencies": {
    "@iconify/json": "^2.2",
    "@iconify/tailwind4": "^1.0",
    "@tailwindcss/forms": "^0.5",
    "preline": "^3.2",
    "webpack": "^5.102",
    "webpack-cli": "^6.0"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1",
    "toastify-js": "^1.12.0"
  }
}
